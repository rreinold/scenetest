{
    "name": "test + - * / %",
    "scenarios": [ ["just try", 1] ],
    "sequencing": "Parallel",
    "just try": {
        "name": "Make sure if statement works",
        "steps": [
            ["set", "foo", 20],
            ["set", "bar", 10],

            ["+", "@foo", "@bar"],
            ["print", "Add:", "@returnValue"],

            ["-", "@foo", "@bar"],
            ["print", "Sub:", "@returnValue"],

            ["*", "@foo", "@bar"],
            ["print", "Mult:", "@returnValue"],

            ["/", "@foo", "@bar"],
            ["print", "Div:", "@returnValue"],

            ["%", "@foo", "@bar"],
            ["print", "Mod:", "@returnValue"]
        ]
    }
}
