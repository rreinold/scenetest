{
    "name": "Name of this test run",
    "scenarios": [ "producer", "producer", "producer", "consumer" ],
    "sequencing": "Parallel",
    "producer": {
        "name": "Spewing out MQTT",
        "steps": [
            ["setUser", "foo@bar.com"],
            ["sleep", 5],
            ["repeat", 10000, 
                [
                    ["publish", "/clearblade/test/spewer", "This is the spew", 2]
                ]
            ]
        ]
    },
    "consumer": {
        "name": "Cleaning up the spew",
        "steps": [
            ["setUser", "bar@bar.com"],
            ["subscribe", "/clearblade/test/spewer", 2],
            ["repeat", 30000, 
                [
                    ["waitMessage", "/clearblade/test/spewer", 30]
                ]
            ]
        ]
    }
}
