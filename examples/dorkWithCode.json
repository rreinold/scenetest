{
    "name": "Muck with code",
    "scenarios": [ ["Updater", 1], ["Caller", 1] ],
    "sequencing": "Parallel",
    "Updater": {
        "name": "Just update some code",
        "steps": [
            ["setUser", "foo@bar.com"],
            ["sync", "Ready, Set, Go!", 2],
            ["repeat", 10000, [
                ["updateService", "HorribleService", "@HorribleService1.js", ["paramXXX"]],
                ["sync", "IChangedIt", 2],
                ["updateService", "HorribleService", "@HorribleService2.js", ["paramYYY"]],
                ["sync", "IChangedIt", 2],
                ["updateService", "HorribleService", "@HorribleService3.js", ["paramZZZ"]],
                ["sync", "IChangedIt", 2] ]
            ],
            ["sync", "Finished", 2]
        ]
    },
    "Caller": {
        "name": "Just callin' some code",
        "steps": [
            ["setUser", "foo@bar.com"],
            ["sync", "Ready, Set, Go!", 2],
            ["repeat", 30000, [
                ["sync", "IChangedIt", 2],
                ["call", "HorribleService", {}],
                ["print", "@returnValue"] ]
            ],
            ["sync", "Finished", 2]
        ]
    }
}
