{
    "name": "Simple Querying",
    "scenarios": [ ["setup", 1], ["scenario1", 10] ],
    "sequencing": "Parallel",
    "setup": [
        ["setUser", "foo@bar.com"],
        ["repeat", 300,
            [
                ["createItem", "YaddaYadda", {"yickety": "blabber", "yak": "blubber"}]
            ]
        ],
        ["sync", "Done Creating", 11]
    ],
    "scenario1": [
        ["setUser", "foo@bar.com"],
        ["sync", "Done Creating", 11],
        ["repeat", 300,
            [
                ["query", "YaddaYadda", [], [[["yickety", "==", "blabber"]]]],
                ["query", "YaddaYadda", [], [[["yak", "==", "blubber"]]]]
            ]
        ]
    ]
}
